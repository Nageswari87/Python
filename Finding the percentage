if __name__ == '__main__':
    n = int(input())
    student_marks = {}
    for _ in range(n):
        name, *line = input().split()
        scores = list(map(float, line))
        student_marks[name] = scores
    query_name = input()
    m=student_marks.get(query_name)
    s=sum(m)
    res=float(s/len(m))

    print(f"{res:.2f}")
